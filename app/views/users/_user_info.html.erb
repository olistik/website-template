<div class="crud_form">
<tr>
    <td>
        <%= link_to "Edit", edit_user_path(user_info) %>
    </td>
    <td>
        <%= link_to "Show", user_path(user_info) %>
    </td>
    <td>
        <%= link_to "Delete", user_path(user_info), :method => :delete %>
    </td>
    <td>
        <%= user_info.id %>
    </td>
    <td>
        <%= user_info.login %>
    </td>
    <td>
        <%= user_info.email %>
    </td>
</tr>
</div>
