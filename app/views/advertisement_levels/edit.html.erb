<div class="crud_form">
<h1>Editing advertisement_level</h1>

<% form_for(@advertisement_level) do |f| %>
    <%= f.error_messages %>

    <p>
        <%= f.label :name %><br />
        <%= f.text_field :name %>
    </p>

    <p>
        <%= f.submit 'Update' %>
    </p>
<% end %>

<%= link_to 'Show', @advertisement_level %> |
<%= link_to 'Back', advertisement_levels_path %>
</div>
