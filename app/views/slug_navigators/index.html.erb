<div class="crud_form">
<h1>Listing slug_navigators</h1>

<%= link_to 'New slug_navigator', new_slug_navigator_path %>
<table>
  <tr>
    <th>Slug</th>
    <th>Parent</th>
    <th>Alt</th>
    <th>Content</th>
    <th colspan="3">Azioni</th>
  </tr>

<% @slug_navigators.each do |slug_navigator| %>
  <tr>
    <td><%=h slug_navigator.slug_id %></td>
    <td><%=h slug_navigator.parent_id %></td>
    <td><%=h slug_navigator.alt %></td>
    <td><%=h slug_navigator.content %></td>
    <td><%= link_to 'Show', slug_navigator %></td>
    <td><%= link_to 'Edit', edit_slug_navigator_path(slug_navigator) %></td>
    <td><%= link_to 'Destroy', slug_navigator, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New slug_navigator', new_slug_navigator_path %>
</div>
